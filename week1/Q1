#include <bits/stdc++.h>
using namespace std;

int main() {
    int a,b,c,i,j,k,size,sum,x,count=0;
cin>>size >>x;
int arr[size];
for(i=0;i<size;i++){
    cin>>arr[i];
}
for(j=0;j<size;j++){
    sum=arr[j];
    if(sum==x){
        count+=1;
        continue;
    }
    else{for(k=j+1;k<size;k++){
        sum=sum+arr[k];
    if(sum==x){
        count+=1;
        break;
    };
    };
    };
    
}
cout<<count;
return 0;
}
